<!-- This event will be handled by component which wants to use Instrument Info. Creating practice order is a component that can use this information -->
<aura:event type="COMPONENT" description="Create Practice Order">
    <!-- Instrument ID to be used; like when creating order -->
    <aura:attribute type="String" name="instrumentID" />
    <!-- Instrument name can be used just to display  -->
    <aura:attribute type="String" name="instrumentName" />
</aura:event>
